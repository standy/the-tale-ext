{
  "extends": "standard",
  "env": {
	"browser": true
  },
  "rules": {
	"comma-dangle": ["error", "always-multiline"],
	"prefer-const": "error",
	"no-var": "error",
	"camelcase": "off",
	"indent": ["error", "tab"],
	"no-tabs": "off",
	"semi": ["error", "always"],
	"spaced-comment": "off",
	"space-before-function-paren": ["error", "never"],
	"no-multiple-empty-lines": ["error", {"max": 3}],
	"linebreak-style": ["error", "windows"]
  }
}
